case $machine in
  badger | fog | grizzly | kodiak | snow | woodchuck | cyclone | fire | ice )
    compiler_list="gcc/system,gcc/8.3.0,intel/19.0.4,pgi/18.10,pgi/19.10"
    ;;
  trinitite | trinity )
    compiler_list="gcc/8.3.0,intel/19.0.4"
    ;;
  viewmaster3 )
    compiler_list="gcc/system"
    ;;
esac

